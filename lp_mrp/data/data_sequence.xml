<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- Sequences for Product Lot --> 
        <record id="seq_product_lot" model="ir.sequence">
            <field name="name">Product Lot</field>
            <field name="code">product.lot</field>
            <field name="prefix">VN%(y)sSC-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_lot_shipped" model="ir.sequence">
            <field name="name">Lot to be Shipped</field>
            <field name="code">lot.shipped</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

